<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.netease.yunxin.kit.contactkit.ui">

    <application>
        <provider
            android:name="com.netease.yunxin.kit.corekit.startup.InitializationProvider"
            android:authorities="${applicationId}.xkit-startup"
            android:exported="false"
            tools:node="merge">
            <meta-data
                android:name="com.netease.yunxin.kit.contactkit.ui.ContactUIService"
                android:value="xkit.startup" />
        </provider>

        <activity
            android:name=".addfriend.AddFriendActivity"
            android:exported="false" />


        <activity
            android:name=".blacklist.BlackListActivity"
            android:screenOrientation="portrait"
            android:launchMode="singleTop" />

        <activity
            android:name=".team.TeamListActivity"
            android:screenOrientation="portrait"
            android:launchMode="singleTop" />

        <activity
            android:name=".verify.VerifyListActivity"
            android:screenOrientation="portrait"
            android:launchMode="singleTop" />

        <activity
            android:name=".userinfo.UserInfoActivity"
            android:screenOrientation="portrait"
            android:launchMode="singleTop" />

        <activity
            android:name=".userinfo.CommentActivity"
            android:screenOrientation="portrait"
            android:launchMode="singleTop" />

        <activity
            android:name=".selector.ContactSelectorActivity"
            android:screenOrientation="portrait"
            android:launchMode="singleTop" />

    </application>

</manifest>